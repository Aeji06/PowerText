{
  "name": "ChromeAutoTextExpander",
  "description": "Create custom keyboard shortcuts to expand and replace text as you type!",
  "author": "Carlin Yuen",
  "private": true,
  "license": "BSD",
  "version": "2.0.0",
  "engines": {
    "node": ">=0.8.0"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true
    },
    "globals": {
      "chrome": true
    },
    "rules": {
      "eol-last": 0,
      "quotes": [
        2,
        "single"
      ]
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "del": "^4.1.1",
    "gulp": "^3.9.1",
    "gulp-cache": "^0.4.3",
    "gulp-chrome-manifest": "0.0.13",
    "gulp-clean-css": "^2.0.3",
    "gulp-eslint": "^2.0.0",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^3.0.0",
    "gulp-livereload": "^3.8.1",
    "gulp-load-plugins": "^1.2.0",
    "gulp-htmlmin": "^2.0.0",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "gulp-useref": "^3.0.8",
    "gulp-zip": "^3.2.0",
    "mocha": "^6.1.4",
    "run-sequence": "^1.1.5",
    "wiredep": "^4.0.0"
  },
  "dependencies": {
  }
}

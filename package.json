{
  "name": "ChromeAutoTextExpander",
  "description": "Create custom keyboard shortcuts to expand and replace text as you type!",
  "author": "Carlin Yuen",
  "private": true,
  "license": "BSD",
  "version": "2.0.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true
    },
    "globals": {
      "@babel/core": "^7.4.5",
      "@babel/preset-env": "^7.4.5",
      "@babel/register": "^7.4.4",
      "chrome": true
    },
    "rules": {
      "eol-last": 0,
      "quotes": [
